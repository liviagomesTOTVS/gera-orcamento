<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.orcamentototvsjakarta.TelaOrcamentosController">
    <children>
        <TableView fx:id="tableOrcamentos" layoutX="20" layoutY="20" prefWidth="600" prefHeight="400">
            <columns>
                <TableColumn fx:id="colNumero" text="Número Orçamento" prefWidth="100"/>
                <TableColumn fx:id="colData" text="Data" prefWidth="150"/>
                <TableColumn fx:id="colValorTotal" text="Valor Total" prefWidth="150"/>
                <TableColumn fx:id="colCliente" text="Cliente" prefWidth="100"/>
                <TableColumn fx:id="colUsuario" text="Usuário" prefWidth="100"/>
            </columns>
        </TableView>
        <Button fx:id="btnVoltar" layoutX="20" layoutY="430" text="Voltar" onAction="#onVoltar"/>
    </children>
</AnchorPane>

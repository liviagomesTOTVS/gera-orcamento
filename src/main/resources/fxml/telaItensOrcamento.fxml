<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.orcamentototvsjakarta.TelaItensOrcamentoController">
    <children>
        <TableView fx:id="tableItens" prefWidth="800.0" prefHeight="400.0" xmlns:fx="http://javafx.com/fxml" >
            <columns>
                <TableColumn fx:id="colCodigo" text="Código" prefWidth="80.0"/>
                <TableColumn fx:id="colDescricao" text="Produto" prefWidth="200.0"/>
                <TableColumn fx:id="colQuantidade" text="Qtde" prefWidth="80.0"/>
                <TableColumn fx:id="colCusto" text="Custo" prefWidth="100.0"/>
                <TableColumn fx:id="colVenda" text="Preço Venda" prefWidth="100.0"/>
                <TableColumn fx:id="colTabela" text="Preço Tabela" prefWidth="100.0"/>
                <TableColumn fx:id="colCodst" text="Tributação (CODST)" prefWidth="100.0"/>
            </columns>
        </TableView>

        <Button fx:id="btnVoltar" layoutX="20" layoutY="430" text="Voltar" onAction="#onVoltar"/>
    </children>
</AnchorPane>

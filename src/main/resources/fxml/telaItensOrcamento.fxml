<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="554.0" prefWidth="878.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.orcamentototvsjakarta.TelaItensOrcamentoController">
    <children>
        <TableView fx:id="tableItens" layoutX="1.0" layoutY="66.0" prefHeight="422.0" prefWidth="876.0" style="-fx-background-color: #d3e4fe;" xmlns:fx="http://javafx.com/fxml">
            <columns>
                <TableColumn fx:id="colCodigo" prefWidth="52.0" text="Código" />
                <TableColumn fx:id="colDescricao" prefWidth="305.0" text="Produto" />
                <TableColumn fx:id="colQuantidade" prefWidth="65.0" text="Qtde" />
                <TableColumn fx:id="colCusto" prefWidth="60.0" text="Custo" />
                <TableColumn fx:id="colVenda" prefWidth="98.0" text="Preço Venda" />
                <TableColumn fx:id="colTabela" prefWidth="108.0" text="Preço Tabela" />
                <TableColumn fx:id="colCodst" minWidth="0.0" text="Tributação (CODST)" />
                <TableColumn fx:id="colTotal" prefWidth="107.0" text="Total" />
            </columns>
        </TableView>

        <Button fx:id="btnVoltar" layoutX="801.0" layoutY="513.0" onAction="#onVoltar" style="-fx-background-color: #0041a6;" text="Voltar" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Pane prefHeight="66.0" prefWidth="878.0" style="-fx-background-color: #0041a6;">
         <children>
            <Text fill="WHITE" layoutX="14.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Itens Orçamento">
               <font>
                  <Font name="Arial Bold" size="24.0" />
               </font>
            </Text>
         </children></Pane>
    </children>
</AnchorPane>
